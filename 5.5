#include <stdio.h>

int main() {
    int number;

    // รับค่าจากผู้ใช้
    printf("Input :\n");
    scanf("%d", &number);

    // ตรวจสอบว่าเป็นเลขคู่หรือเลขคี่
    if (number % 2 == 0) {
        printf("%d is an even integer\n", number);
    } else {
        printf("%d is an odd integer\n", number);
    }

    return 0;
}
